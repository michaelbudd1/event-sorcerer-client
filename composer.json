{
    "name": "eventsourcerer/event-sourcerer-client",
    "description": "A PHP library to interact with Event Sourcerer",
    "type": "library",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "PearTreeWeb\\EventSourcerer\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "michaelbudd",
            "email": "michaelbudd6@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "symfony/http-client": "7.2.*",
        "revolt/event-loop": "^1.0",
        "react/socket": "^1.16",
        "eventsourcerer/message-utilities": "dev-main",
        "eventsourcerer/symfony-messenger-transport": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    }
}
