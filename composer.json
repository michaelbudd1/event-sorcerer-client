{
    "name": "eventsourcerer/event-sourcerer-client",
    "description": "A PHP library to interact with Event Sourcerer",
    "type": "library",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "PearTreeWeb\\EventSourcerer\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "michaelbudd",
            "email": "michaelbudd6@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "symfony/http-client": "8.0.*",
        "revolt/event-loop": "^1.0",
        "react/socket": "^1.17",
        "eventsourcerer/message-utilities": "dev-main",
        "react/async": "^4.3.0",
        "psr/cache": "^3.0",
        "symfony/cache": "8.0.*",
        "symfony/lock": "8.0.*",
        "illuminate/collections": "^12.49",
        "react/event-loop": "^1.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.5.8",
        "symfony/yaml": "8.0.*"
    }
}
